{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "lint": "eslint app --fix",
    "clean": "rm -rf build public/build .cache .turbo node_modules",
    "prisma:migrate": "npx prisma migrate dev --name",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test dev http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/server/index.js",
    "/public/build"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^6.0.2",
    "@mantine/hooks": "^6.0.2",
    "@mantine/remix": "^6.0.2",
    "@prisma/client": "^4.11.0",
    "@react-sigma/core": "^3.3.0",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@remix-run/serve": "^1.14.3",
    "bcryptjs": "^2.4.3",
    "graphology": "^0.25.1",
    "isbot": "^3.6.7",
    "react": "^18.2.0",
    "react-auth": "^1.0.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-utils": "^6.0.0",
    "sigma": "^2.4.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@mermaid-js/mermaid-cli": "^10.0.2",
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "c8": "^7.13.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-plugin-cypress": "^2.12.1",
    "graphology-types": "^0.24.7",
    "happy-dom": "^8.9.0",
    "msw": "^1.1.0",
    "prisma": "^4.11.0",
    "prisma-erd-generator": "^1.2.5",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.8.4",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.3"
  },
  "engines": {
    "node": ">=14"
  }
}
